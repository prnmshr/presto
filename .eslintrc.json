{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "Feature": "readonly",
        "Before": "readonly",
        "Scenario": "readonly",
        "locate": "readonly",
        "assert": "readonly",
        "describe": "readonly",
        "it": "readonly",
        "expect": "readonly"



    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0
    }
}
